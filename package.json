{
  "name": "aertec-jobtest",
  "version": "1.0.0",
  "description": "Programming test for a junior Front End Developer position in [Aertec Solutions](https://aertecsolutions.com/).",
  "main": "main.tsx",
  "homepage": "https://firenz.github.io/aertec-jobtest/",
  "author": "Alicia G. <alicia.guardenoalbertos@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jobtest",
    "react",
    "react-query",
    "typescript",
    "material-ui",
    "webpack",
    "babel",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "start": "rimraf dist && webpack-dev-server --mode development --open --config dev.webpack.config.js",
    "build:dev": "rimraf dist && webpack --mode development --config dev.webpack.config.js",
    "build:prod": "rimraf dist && webpack --mode production --config prod.webpack.config.js",
    "build:stats": "rimraf dist && webpack --mode development --config perf.webpack.config.js",
    "deploy:gh-pages": "npm run build:prod && gh-pages-clean && gh-pages -d dist -r https://$github_token@github.com/Firenz/aertec-jobtest.git",
    "test": "jest --verbose -c ./config/test/jest.json",
    "test:watch": "npm test -- --watchAll -i --no-cache",
    "test:coverage": "rimraf coverage && jest -c ./config/test/jest.coverage.json --verbose",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firenz/aertec-jobtest.git"
  },
  "bugs": {
    "url": "https://github.com/Firenz/aertec-jobtest/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@imaginary-cloud/eslint-config-react": "^1.0.1",
    "@imaginary-cloud/prettier-config": "^1.0.0",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "chunks-webpack-plugin": "^6.1.0",
    "css-loader": "^4.2.2",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.1.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.0",
    "prettier": "^2.1.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
